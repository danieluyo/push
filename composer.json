{
    "name" : "genkgo/push",
    "description": "Send push messages to Android, Apple and Windows using one interface ",
    "require" : {
        "php" : ">=5.6",
        "jildertmiedema/windows-phone-push": "~0.0.1",
        "php-gcm/php-gcm": "~1.1.1",
        "apple/apn-push": "~2.2.2"
    },
    "require-dev" : {
        "phpunit/phpunit" : "~4.0",
        "phpunit/phpunit-mock-objects" : "~2.3"
    },
    "autoload" : {
        "psr-4" : {
            "Genkgo\\Push\\" : ["src"]
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Genkgo\\Push\\" : ["tests"]
        }
    }
}